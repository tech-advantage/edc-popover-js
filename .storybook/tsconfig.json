{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "lib": [
      "dom",
      "es2015"
    ],
    "declaration": false,
    "sourceMap": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": [
    "node_modules"
  ]
}
